{ 
  "build": {
    "content": [
      {
        "files": [
          "docfiles/**/*.yml"
        ]
      },
      {
        "files": [
          "docfiles/**/*.md",
          "toc.yml",
          "**/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docfiles/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "docfiles/**/*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default","rules"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}